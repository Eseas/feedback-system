<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lt.vu.dao.datamapper.UserSurveyMapper">

  <resultMap id="BaseResultMap" type="lt.vu.entities.mybatis.UserSurvey">
    <id column="user_id" jdbcType="INTEGER" property="userId" />
    <id column="survey_id" jdbcType="INTEGER" property="surveyId" />
  </resultMap>

  <delete id="deleteByPrimaryKey" parameterType="map">
    delete from public.user_survey
    where user_id = #{userId,jdbcType=INTEGER}
      and survey_id = #{surveyId,jdbcType=INTEGER}
  </delete>

  <insert id="insert" parameterType="UserSurvey">
    insert into public.user_survey (user_id, survey_id)
    values (#{userId,jdbcType=INTEGER}, #{surveyId,jdbcType=INTEGER})
  </insert>

  <select id="selectAll" resultMap="BaseResultMap">
    select user_id, survey_id
    from public.user_survey
  </select>
</mapper>